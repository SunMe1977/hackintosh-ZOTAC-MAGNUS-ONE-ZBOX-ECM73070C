            <key>RebuildAppleMemoryMap</key><true/>
            <key>ResizeAppleGpuBars</key><integer>-1</integer>
            <key>SyncRuntimePermissions</key><true/>
        </dict>
    </dict>

    <key>DeviceProperties</key>
    <dict>
        <key>Add</key><dict/>
        <key>Delete</key><dict/>
    </dict>

